  "license": "GPL-3.0-or-later",
  "dependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "hsluv": "^0.0.3",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.12.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-dropdown": "^1.6.4",
    "react-input-range": "^1.3.0",
    "react-redux": "^7.1.3",
    "react-tabs": "^3.1.0",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-tabs": "^2.3.1",
    "@types/react-test-renderer": "^16.9.1",
    "node-sass-chokidar": "^1.4.0",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4",
    "yarn-run-all": "^3.1.1"
  },
  "scripts": {
    "watch": "npm-run-all --parallel start css test",
    "start": "react-scripts start",
    "css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "npm run build-css && react-scripts test",
    "build": "npm-run-all build-css build-react",
    "build-react": "react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "serve": "ln -fs . build/hexerals && serve -s build -o",
    "build-serve": "npm-run-all build serve",
    "eject": "react-scripts eject",
    "compile": "tsc --noEmit false --outDir pack --declaration --isolatedModules false",
    "verbose": "tsc --noEmit false --outDir pack --declaration --isolatedModules false --extendedDiagnostics"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
